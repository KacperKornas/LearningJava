<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.datepicker.Controller">
   <children>
      <Label fx:id="myLabel" alignment="CENTER" layoutX="136.0" layoutY="104.0" prefHeight="45.0" prefWidth="329.0" text="Date">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <DatePicker fx:id="myDatePicker" layoutX="213.0" layoutY="229.0" onAction="#showDate" promptText="Enter a date" />
   </children>
</AnchorPane>
