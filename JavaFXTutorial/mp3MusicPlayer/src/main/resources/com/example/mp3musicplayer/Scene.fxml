<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="161.0" prefWidth="632.0" style="-fx-background-color: #222222;" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mp3musicplayer.Controller">
   <children>
      <Label fx:id="songLabel" alignment="CENTER" layoutX="-1.0" layoutY="-1.0" prefHeight="92.0" prefWidth="632.0" text="MP3 PLAYER" textFill="#22ab00">
         <font>
            <Font size="35.0" />
         </font>
      </Label>
      <ProgressBar fx:id="songProgressBar" layoutX="4.0" layoutY="90.0" prefHeight="18.0" prefWidth="625.0" progress="0.0" />
      <Button fx:id="playButton" layoutX="6.0" layoutY="108.0" mnemonicParsing="false" onAction="#playMedia" prefHeight="51.0" prefWidth="81.0" text="PLAY">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button fx:id="pauseButton" layoutX="87.0" layoutY="108.0" mnemonicParsing="false" onAction="#pauseMedia" prefHeight="51.0" prefWidth="81.0" text="PAUSE">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button fx:id="resetButton" layoutX="168.0" layoutY="108.0" mnemonicParsing="false" onAction="#resetMedia" prefHeight="51.0" prefWidth="81.0" text="RESET">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button fx:id="previousButton" layoutX="249.0" layoutY="108.0" mnemonicParsing="false" onAction="#previousMedia" prefHeight="51.0" prefWidth="81.0" text="PREVIOUS">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button fx:id="nextButton" layoutX="330.0" layoutY="108.0" mnemonicParsing="false" onAction="#nextMedia" prefHeight="51.0" prefWidth="81.0" text="NEXT">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <ComboBox fx:id="speedBox" layoutX="411.0" layoutY="108.0" onAction="#changeSpeed" prefHeight="51.0" prefWidth="81.0" promptText="SPEED" />
      <Slider fx:id="volumeSlider" layoutX="492.0" layoutY="127.0" max="200.0" value="100.0" />
   </children>
</AnchorPane>
